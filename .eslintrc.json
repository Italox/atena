{
  "plugins": ["prettier"],
  "extends": ["prettier"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "modules": true,
      "experimentalObjectRestSpread": true
    }
  },
  "env": {
    "es6": true,
    "node": true,
    "browser": true,
    "jest": true
  },
  "rules": {
    "strict": 0,
    "no-console": "off",
    "indent": ["error", 2, {
      "ImportDeclaration": "first",
      "MemberExpression": 1,
      "SwitchCase": 1
    }],
    "prettier/prettier": "error"
  }
}


