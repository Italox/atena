{
  "plugins": ["prettier"],
  "extends": ["prettier"],
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "node": true,
    "browser": true,
    "jest": true
  },
  "rules": {
    "strict": 0,
    "no-console": "off",
    "indent": ["error", 2],
    "indent-size": 2,
    "prettier/prettier": "error"
  }
}
